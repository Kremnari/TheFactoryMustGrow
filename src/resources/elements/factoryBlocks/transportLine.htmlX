<template bindable="line, type, upgrades, invClick">
  <div if.bind="type=='output'">
  </div>
  <div if.bind="type=='internal'">Internal</div>
  <icon-base
    repeat.for="item of line.items"
    item.bind="item"
    count.bind="item.count"
    click.delegate="invClick({inv: line, item})"
  ></icon-base>
  <div if.bind="type=='input'">
  </div>
</template>
